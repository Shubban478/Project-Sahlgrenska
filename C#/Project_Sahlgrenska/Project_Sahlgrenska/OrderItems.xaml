        <CheckBox x:Name="Inderal" Content="Inderal" HorizontalAlignment="Left" Margin="10,474,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="Julmust" Content="Julmust" HorizontalAlignment="Left" Margin="10,494,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="Kokain" Content="Kokain" HorizontalAlignment="Left" Margin="10,514,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="Snus" Content="Snus" HorizontalAlignment="Left" Margin="10,534,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="Blodtrycksmaskin" Content="Blodtrycksmaskin" HorizontalAlignment="Left" Margin="201,434,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="Ultraljudsmaskin" Content="Ultraljudsmaskin" HorizontalAlignment="Left" Margin="201,454,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="EKG" Content="EKG" HorizontalAlignment="Left" Margin="201,474,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="Hjärtstartare" Content="Hjärtstartare" HorizontalAlignment="Left" Margin="201,494,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="Bladderscan" Content="Bladderscan" HorizontalAlignment="Left" Margin="201,514,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="Termometer" Content="Termometer" HorizontalAlignment="Left" Margin="201,534,0,0" VerticalAlignment="Top"/>
