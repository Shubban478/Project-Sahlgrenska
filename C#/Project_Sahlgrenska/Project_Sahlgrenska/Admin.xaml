<Window x:Class="Project_Sahlgrenska.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_Sahlgrenska"
        mc:Ignorable="d"
        Name="FirstWindow" Icon="/img/Icon1.png"
        Title="Admin" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Update"/>
            <TextBox x:Name="update" KeyDown="update_KeyDown" />
            <Label Content="Select"/>
            <TextBox x:Name="select" KeyDown="select_KeyDown"/>
            <Label Content="Output"/>
            <DataGrid x:Name="output" AutoGenerateColumns="True" IsReadOnly="True"/>
        </StackPanel>
    </Grid>
</Window>
